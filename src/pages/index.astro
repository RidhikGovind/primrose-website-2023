---
import BaseLayout from '../layouts/BaseLayout.astro';
import BannerFooterSection from '../components/BannerFooterSection.astro';
import ButtonOutlined from '../components/ButtonOutlined.astro';
import SupportBannerWithPic from '../components/SupportBannerWithPic.astro';
---

<script
	type="text/javascript"
	src="https://assets.calendly.com/assets/external/widget.js"
></script>

<style></style>

<BaseLayout
	title="Primrose.health | We help medical practices through the riskiest part of their journey"
>
	<main>
		<div class="container md:max-w-full">
			<!-- Hero -->
			<div
				class="max-w-[85rem] mt-8 md:mt-0 md:max-w-full mx-auto pt-10 pb-10 px-4 sm:px-6 lg:px-8"
			>
				<!-- Grid -->
				<div
					class="grid md:grid-cols-2 gap-12 md:gap-16 xl:gap-20 md:items-center"
				>
					<div>
						<h1
							class="block text-5xl tracking-tighter font-bold text-gray-800 sm:text-4xl lg:text-7xl lg:leading-tight dark:text-white"
						>
							No hassle <span class="">start-up clinic!</span>
						</h1>
						<p class="mt-4 text-lg md:text-xl text-gray-800 dark:text-gray-400">
							We help medical practices through the riskiest part of their
							journey - the first year. Benefit from our experience, avoid
							common pitfalls and get to profitability in the shortest period of
							time.
						</p>

						<!-- Buttons -->
						<div class="mt-8 grid gap-3 w-full sm:inline-flex">
							<a
								class="inline-flex calendly-open-button justify-center items-center gap-x-3 text-center bg-teal-700 hover:bg-teal-800 border border-transparent text-sm lg:text-base text-white font-medium transition py-3 px-4 dark:focus:ring-offset-gray-800"
								onclick="openCalendlyWidget()"
								href="#calendly"
							>
								Schedule a call with an expert
							</a>
						</div>
						<!-- End Buttons -->
					</div>
					<!-- End Col -->

					<div class="relative scale-in-ver-center">
						<img
							class="w-full max-w-full"
							src="/images/hero-img-desktop-2.jpg"
							alt="Image Description"
						/>
					</div>
					<!-- End Col -->
				</div>
				<!-- End Grid -->
			</div>
			<!-- End Hero -->

			<!-- Features -->

			<div
				class="bg-neutral-100 md:max-w-full px-4 py-10 lg:py-14 md:px-0 mx-auto"
			>
				<div class="max-w-[85rem] mx-auto">
					<!-- Title -->
					<div class="mx-auto max-w-2xl text-center">
						<h2
							class="text-4xl lg:text-5xl text-gray-900 font-bold dark:text-gray-200 px-8 md:py-16"
						>
							Built for <span
								class="text-emerald-600 underline decoration-4 decoration-black underline-offset-8 decoration-solid"
							>
								startup clinics</span
							>
						</h2>
					</div>
					<!-- End Title -->

					<div class="grid grid-col-6 gap-24 md:gap-12 px-6 py-8">
						<!-- Grid - Feature 1 -->
						<div
							class="grid gap-4 md:grid-cols-2 md:items-center md:gap-12 xl:gap-32 mt-6"
						>
							<div>
								<img
									class=""
									src="/images/credentialing-homepage.png"
									alt="Image Description"
								/>
							</div>
							<!-- End Col -->

							<div class="mt-5 sm:mt-10 lg:mt-0">
								<div class="space-y-6 sm:space-y-8">
									<!-- Title -->
									<div class="space-y-2 md:space-y-4">
										<h2
											class="font-bold text-2xl lg:text-4xl text-gray-800 dark:text-gray-200"
										>
											Credentialing
										</h2>
										<p class="text-gray-500 md:text-xl">
											Getting contracted with payers quickly is critical to get
											cash flow positive. Our credentialing experts can navigate
											the often confusing contracting process.
										</p>

										<!-- Button with Arrow -->
										<a
											class="group flex gap-y-6 w-full h-full transition-all"
											href="/credentialing"
										>
