<script>
	const calendlyCloseButton = document.querySelector('.calendly-close-button');

	calendlyCloseButton.addEventListener('click', () => {
		document.querySelector('.calendly-widget-overlay').style.display = 'none';
	});

	const calendlyOpenButton = document.querySelectorAll('.calendly-open-button');

	// Handle clicks on each button.
	calendlyOpenButton.forEach((button) => {
		button.addEventListener('click', () => {
			document.querySelector('.calendly-widget-overlay').style.display =
				'block';
		});
	});
</script>
