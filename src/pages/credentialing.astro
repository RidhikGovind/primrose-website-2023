---
import BaseLayout from '../layouts/BaseLayout.astro';
import BannerFooterSection from '../components/BannerFooterSection.astro';
import SupportBannerWithList from '../components/SupportBannerWithList.astro';
import ButtonOutlined from '../components/ButtonOutlined.astro';
---

<BaseLayout title="Credentialing | Primrose.health">
	<main>
		<div class="container md:max-w-full">
			<!-- Hero -->
			<div
				class="max-w-[85rem] md:max-w-[100rem] mx-auto pt-10 pb-10 px-4 sm:px-6 lg:px-8 bg-neutral-100"
			>
				<!-- Grid -->
				<div
					class="grid md:grid-cols-2 gap-12 md:gap-16 xl:gap-20 md:items-center"
				>
					<div>
						<h1
							class="block text-4xl tracking-tighter font-bold text-gray-800 sm:text-4xl lg:text-6xl lg:leading-tight dark:text-white"
						>
							Credentialing
						</h1>
						<p class="mt-4 text-lg text-gray-800 dark:text-gray-400">
							Getting contracted with payers quickly is critical to get cash
							flow positive. Our credentialing experts can navigate the often
							confusing contracting process.
						</p>

						<!-- Buttons -->
						<div class="mt-8 grid gap-3 w-full sm:inline-flex">
							<a
								class="inline-flex justify-center items-center gap-x-3 text-center bg-teal-700 hover:bg-teal-800 border border-transparent text-sm lg:text-base text-white font-medium focus:outline-none focus:ring-2 focus:ring-blue-600 focus:ring-offset-2 focus:ring-offset-white transition py-3 px-4 dark:focus:ring-offset-gray-800"
								href="#"
							>
								Get started
								<!-- <svg
									class='w-2.5 h-2.5'
									width='16'
									height='16'
									viewBox='0 0 16 16'
									fill='none'
								>
									<path
										d='M5.27921 2L10.9257 7.64645C11.1209 7.84171 11.1209 8.15829 10.9257 8.35355L5.27921 14'
										stroke='currentColor'
										stroke-width='2'
										stroke-linecap='round'></path>
								</svg> -->
							</a>
						</div>
						<!-- End Buttons -->
					</div>
					<!-- End Col -->

					<div class="relative">
						<img
							class="w-full max-w-full"
							src="/images/credentialing-hero.png"
							alt="Image Description"
						/>
						<!-- <div
							class='absolute inset-0 -z-[1] bg-gradient-to-tr from-gray-200 via-white/0 to-white/0 w-full h-full rounded-md mt-4 -mb-4 mr-4 -ml-4 lg:mt-6 lg:-mb-6 lg:mr-6 lg:-ml-6 dark:from-slate-800 dark:via-slate-900/0 dark:to-slate-900/0'
						>
						</div> -->
					</div>
					<!-- End Col -->
				</div>
				<!-- End Grid -->
			</div>
			<!-- End Hero -->

			<!-- Time capsule banner start -->
			<div class="max-w-[85rem] px-4 pt-10 sm:px-6 lg:px-8 lg:py-14 mx-auto">
				<div
					class="bg-[#E0DDD8] sm:p-12 py-12 px-6 grid grid-rows-2 md:gap-12 gap-6"
				>
					<div class="justify-self-end max-w-[25rem]">
						<h2
							class="font-bold text-2xl tracking-tighter lg:text-3xl text-gray-800 dark:text-gray-200 mb-4"
						>
							We save time. Period.
						</h2>
						<p class="text-gray-900">
							Streamline your credentialing process with our time-saving
							solutions. Experience efficiency.
						</p>
					</div>

					<h3
						class="md:text-8xl text-7xl font-bold text-gray-800 tracking-tighter"
					>
						60 minutes
					</h3>
				</div>
			</div>
			<!-- Time capsule banner end -->

			<!-- Features -->
			<div class="max-w-[85rem] px-4 py-10 sm:px-6 lg:px-8 lg:py-14 mx-auto">
				<div class="bg-white">
					<!-- Title -->
					<div class="mx-auto max-w-2xl text-center">
						<h2
							class="text-3xl lg:text-4xl text-gray-800 font-bold dark:text-gray-200 px-8 pt-8"
						>
							Ditch the old way. <br /> Try Primrose today.
						</h2>
						<p class="text-center text-gray-800 dark:text-gray-200 px-8 pt-8">
							Lorem ipsum dolor sit amet, consectetur adipiscing elit,sed do
							eiusmod tempor incididunt ut labore et dolore magna aliqua.
						</p>
					</div>
					<!-- End Title -->

					<div class="grid grid-col-6 gap-12 px-4 py-8">
						<!-- Grid - Feature 1 -->
						<div
							class="md:grid md:grid-cols-2 md:items-center md:gap-12 xl:gap-32 mt-6"
						>
							<div>
								<img
									class=""
									src="/images/npi-registration.png"
									alt="Image Description"
								/>
							</div>
							<!-- End Col -->

							<div class="mt-5 sm:mt-10 lg:mt-0">
								<div class="space-y-6 sm:space-y-8">
									<!-- Title -->
									<div class="space-y-2 md:space-y-4">
										<h2
											class="font-bold text-2xl lg:text-3xl text-gray-800 dark:text-gray-200"
										>
											NPI Registration
										</h2>
										<p class="text-gray-500">
											If you have not gotten a NPI for your entity, that is the
											very first step. We are able to register and get your
											organization an NPI number fairly quickly.
										</p>
										<!-- <span
											class="mt-2 inline-flex items-center gap-x-1.5 text-blue-600 decoration-2 group-hover:underline font-medium"
										>
											Learn more
											<svg
												class="w-2.5 h-2.5"
												width="16"
												height="16"
												viewBox="0 0 16 16"
												fill="none"
											>
												<path
													d="M5.27921 2L10.9257 7.64645C11.1209 7.84171 11.1209 8.15829 10.9257 8.35355L5.27921 14"
													stroke="currentColor"
													stroke-width="2"
													stroke-linecap="round"></path>
											</svg>
										</span> -->
									</div>
									<!-- End Title -->
								</div>
							</div>
							<!-- End Col -->
						</div>
						<!-- End Grid -->

						<!-- Grid - Feature 2 -->
						<div
							class="md:grid md:grid-cols-2 md:items-center md:gap-12 xl:gap-32"
						>
							<div>
								<img
									class=""
									src="/images/medicare-medicaid.png"
									alt="Image Description"
								/>
							</div>
							<!-- End Col -->

							<div class="mt-5 sm:mt-10 lg:mt-0">
								<div class="space-y-6 sm:space-y-8">
									<!-- Title -->
									<div class="space-y-2 md:space-y-4">
										<h2
											class="font-bold text-2xl lg:text-3xl text-gray-800 dark:text-gray-200"
										>
											Medicare & Medicaid
										</h2>
										<p class="text-gray-500">
											After NPI registration, we start Medicare and Medicaid
											credentialing at no additional cost. Medicare and Medicaid
											can be tricky at times and using a team with substantial
											experience with these two payers can help get them
											completed quickly.
										</p>
									</div>
									<!-- End Title -->
								</div>
							</div>
						</div>
						<!-- End Grid -->

						<!-- Grid - Feature 3 -->
						<div
							class="md:grid md:grid-cols-2 md:items-center md:gap-12 xl:gap-32"
						>
							<div>
								<img class="" src="/images/CAQH.png" alt="Image Description" />
							</div>
							<!-- End Col -->

							<div class="mt-5 sm:mt-10 lg:mt-0">
								<div class="space-y-6 sm:space-y-8">
									<!-- Title -->
									<div class="space-y-2 md:space-y-4">
										<h2
											class="font-bold text-2xl lg:text-3xl text-gray-800 dark:text-gray-200"
										>
											CAQH
										</h2>
										<p class="text-gray-500">
											CAQH is a centralized credentialing database that is used
											by many payors. We create a CAQH profile for each provider
											in your group and will maintain it for your organization.
										</p>
									</div>
									<!-- End Title -->
								</div>
							</div>
							<!-- End Col -->
						</div>
						<!-- End Grid -->

						<!-- Grid - Feature 4 -->
						<div
							class="md:grid md:grid-cols-2 md:items-center md:gap-12 xl:gap-32"
						>
							<div>
								<img
									class=""
									src="/images/ipa-coordination.png"
									alt="Image Description"
								/>
							</div>
							<!-- End Col -->

							<div class="mt-5 sm:mt-10 lg:mt-0">
								<div class="space-y-6 sm:space-y-8">
									<!-- Title -->
									<div class="space-y-2 md:space-y-4">
										<h2
											class="font-bold text-2xl lg:text-3xl text-gray-800 dark:text-gray-200"
										>
											IPA coordination
										</h2>
										<p class="text-gray-500">
											In certain markets, it makes sense to work with an
											Independent Physician Association (IPA) instead of
											credentialing individually. They can often speed up the
											credentialing process by months and can sometimes get
											better rates. We will coordinate activities with your IPA
											to ensure credentialing is on schedule and that you and
											our billing team are kept abreast of the progress.
										</p>
									</div>
									<!-- End Title -->
								</div>
							</div>
							<!-- End Col -->
						</div>
						<!-- End Grid -->

						<!-- Testimonials -->
						<div
							class="relative max-w-[85rem] px-4 py-10 sm:px-6 lg:px-8 lg:py-14 mx-auto bg-neutral-100"
						>
							<!-- Blockquote -->
							<blockquote class="text-center lg:mx-auto lg:w-3/5">
								<img
									class="mx-auto w-20 h-auto sm:w-28 text-gray-800 dark:text-gray-200"
									src="/images/five-star-review.svg"
									alt=""
								/>

								<div class="mt-6 lg:mt-10">
									<p
										class="relative text-xl sm:text-2xl md:leading-normal font-medium text-gray-800"
									>
										<svg
											class="absolute top-0 left-0 transform -translate-x-8 -translate-y-8 h-16 w-16 text-gray-200 sm:h-24 sm:w-24 dark:text-gray-700"
											width="16"
											height="13"
											viewBox="0 0 16 13"
											fill="none"
											xmlns="http://www.w3.org/2000/svg"
											aria-hidden="true"
										>
											<path
												d="M7.18079 9.25611C7.18079 10.0101 6.93759 10.6211 6.45119 11.0891C5.96479 11.5311 5.35039 11.7521 4.60799 11.7521C3.71199 11.7521 2.96958 11.4531 2.38078 10.8551C1.81758 10.2571 1.53598 9.39911 1.53598 8.28111C1.53598 7.08511 1.86878 5.91511 2.53438 4.77111C3.22559 3.60111 4.18559 2.67811 5.41439 2.00211L6.29759 3.36711C5.63199 3.83511 5.09439 4.35511 4.68479 4.92711C4.30079 5.49911 4.04479 6.16211 3.91679 6.91611C4.14719 6.81211 4.41599 6.76011 4.72319 6.76011C5.43999 6.76011 6.02879 6.99411 6.48959 7.46211C6.95039 7.93011 7.18079 8.52811 7.18079 9.25611ZM14.2464 9.25611C14.2464 10.0101 14.0032 10.6211 13.5168 11.0891C13.0304 11.5311 12.416 11.7521 11.6736 11.7521C10.7776 11.7521 10.0352 11.4531 9.44639 10.8551C8.88319 10.2571 8.60159 9.39911 8.60159 8.28111C8.60159 7.08511 8.93439 5.91511 9.59999 4.77111C10.2912 3.60111 11.2512 2.67811 12.48 2.00211L13.3632 3.36711C12.6976 3.83511 12.16 4.35511 11.7504 4.92711C11.3664 5.49911 11.1104 6.16211 10.9824 6.91611C11.2128 6.81211 11.4816 6.76011 11.7888 6.76011C12.5056 6.76011 13.0944 6.99411 13.5552 7.46211C14.016 7.93011 14.2464 8.52811 14.2464 9.25611Z"
												fill="currentColor"></path>
										</svg>
										<span
											class="relative z-10 italic text-gray-800 dark:text-gray-200"
											>“Three months after starting credentialing, I realized my
											applications were not even submitted. Primrose came in,
											took control of the entire process and we never looked
											back. An up to date payor grid, weekly check-in meeting,
											an organized process saw us through. I would not have lost
											revenue if I had started Primrose from the very
											beginning.”</span
										>
									</p>
								</div>

								<footer class="mt-6">
									<div class="font-semibold text-gray-800 dark:text-gray-200">
										Dr. Makinde
									</div>
									<div class="text-sm text-gray-500">
										Director | Covenant Women's Health, Texas
									</div>
								</footer>
							</blockquote>
							<!-- End Blockquote -->
						</div>
						<!-- End Testimonials -->

						<!-- Grid - Feature 5 -->
						<div
							class="md:grid md:grid-cols-2 md:items-center md:gap-12 xl:gap-32"
						>
							<div>
								<img
									class=""
									src="/images/commercial-contracting.png"
									alt="Image Description"
								/>
							</div>
							<!-- End Col -->

							<div class="mt-5 sm:mt-10 lg:mt-0">
								<div class="space-y-6 sm:space-y-8">
									<!-- Title -->
									<div class="space-y-2 md:space-y-4">
										<h2
											class="font-bold text-2xl lg:text-3xl text-gray-800 dark:text-gray-200"
										>
											Commercial contracting
										</h2>
										<p class="text-gray-500">
											Once a decision whether to use IPA or not is made, we
											identify the top 15 payors in your market and start the
											contracting process with any payor not covered by the IPA.
											Commercial credentialing may incur additional costs.
										</p>
									</div>
									<!-- End Title -->
								</div>
							</div>
							<!-- End Col -->
						</div>
						<!-- End Grid -->

						<!-- Grid - Feature 6 -->
						<div
							class="md:grid md:grid-cols-2 md:items-center md:gap-12 xl:gap-32"
						>
							<div>
								<img
									class=""
									src="/images/coordinated-billing.png"
									alt="Image Description"
								/>
							</div>
							<!-- End Col -->
							<div class="mt-5 sm:mt-10 lg:mt-0">
								<div class="space-y-6 sm:space-y-8">
									<!-- Title -->
									<div class="space-y-2 md:space-y-4">
										<h2
											class="font-bold text-2xl lg:text-3xl text-gray-800 dark:text-gray-200"
										>
											Coordinated billing
										</h2>
										<p class="text-gray-500">
											Knowing when to hold a claim and when to file can make all
											the difference between getting paid or getting processed
											out-of-network. Our credentialing team works hand-in-hand
											with the billing department to file claims to maximize
											your collection.
										</p>
									</div>
									<!-- End Title -->
								</div>
							</div>
							<!-- End Col -->
						</div>
						<!-- End Grid -->

						<!-- Grid - Feature 7 -->
						<div
							class="md:grid md:grid-cols-2 md:items-center md:gap-12 xl:gap-32"
						>
							<div>
								<img
									class=""
									src="/images/weekly-meetings.png"
									alt="Image Description"
								/>
							</div>
							<!-- End Col -->
							<div class="mt-5 sm:mt-10 lg:mt-0">
								<div class="space-y-6 sm:space-y-8">
									<!-- Title -->
									<div class="space-y-2 md:space-y-4">
										<h2
											class="font-bold text-2xl lg:text-3xl text-gray-800 dark:text-gray-200"
										>
											Weekly meetings
										</h2>
										<p class="text-gray-500">
											Transition plan is established at the start of the
											onboarding process. We do weekly meetings with clients
											during the implementation phase to go over the transition
											plan and fix any issues.
										</p>
									</div>
									<!-- End Title -->
								</div>
							</div>
							<!-- End Col -->
						</div>
						<!-- End Grid -->
					</div>
				</div>
			</div>
			<!-- End Features -->

			<SupportBannerWithList />

			<!-- FAQ -->
			<div class="max-w-[85rem] px-4 py-10 sm:px-6 lg:px-8 lg:py-14 mx-auto">
				<!-- Title -->
				<div class="max-w-2xl mx-auto text-center mb-10 lg:mb-14">
					<h2
						class="text-2xl font-bold md:text-4xl md:leading-tight dark:text-white"
					>
						Your questions, answered
					</h2>
					<p class="mt-1 text-gray-600 dark:text-gray-400">
						Answers to the most frequently asked questions.
					</p>
				</div>
				<!-- End Title -->

				<div class="max-w-2xl mx-auto">
					<!-- Accordion -->
					<div class="hs-accordion-group">
						<div
							class="hs-accordion hs-accordion-active:bg-gray-100 rounded-xl p-6 dark:hs-accordion-active:bg-white/[.05] active"
							id="hs-basic-with-title-and-arrow-stretched-heading-one"
						>
							<button
								class="hs-accordion-toggle group pb-3 inline-flex items-center justify-between gap-x-3 w-full md:text-lg font-semibold text-left text-gray-800 transition hover:text-gray-500 dark:text-gray-200 dark:hover:text-gray-400"
								aria-controls="hs-basic-with-title-and-arrow-stretched-collapse-one"
							>
								Can I cancel at anytime?
								<svg
									class="hs-accordion-active:hidden block w-3 h-3 text-gray-600 group-hover:text-gray-500 dark:text-gray-400"
									width="16"
									height="16"
									viewBox="0 0 16 16"
									fill="none"
									xmlns="http://www.w3.org/2000/svg"
								>
									<path
										d="M2 5L8.16086 10.6869C8.35239 10.8637 8.64761 10.8637 8.83914 10.6869L15 5"
										stroke="currentColor"
										stroke-width="2"
										stroke-linecap="round"></path>
								</svg>
								<svg
									class="hs-accordion-active:block hidden w-3 h-3 text-gray-600 group-hover:text-gray-500 dark:text-gray-400"
									width="16"
									height="16"
									viewBox="0 0 16 16"
									fill="none"
									xmlns="http://www.w3.org/2000/svg"
								>
									<path
										d="M2 11L8.16086 5.31305C8.35239 5.13625 8.64761 5.13625 8.83914 5.31305L15 11"
										stroke="currentColor"
										stroke-width="2"
										stroke-linecap="round"></path>
								</svg>
							</button>
							<div
								id="hs-basic-with-title-and-arrow-stretched-collapse-one"
								class="hs-accordion-content w-full overflow-hidden transition-[height] duration-300"
								aria-labelledby="hs-basic-with-title-and-arrow-stretched-heading-one"
							>
								<p class="text-gray-800 dark:text-gray-200">
									Yes, you can cancel anytime no questions are asked while you
									cancel but we would highly appreciate if you will give us some
									feedback.
								</p>
							</div>
						</div>

						<div
							class="hs-accordion hs-accordion-active:bg-gray-100 rounded-xl p-6 dark:hs-accordion-active:bg-white/[.05]"
							id="hs-basic-with-title-and-arrow-stretched-heading-two"
						>
							<button
								class="hs-accordion-toggle group pb-3 inline-flex items-center justify-between gap-x-3 w-full md:text-lg font-semibold text-left text-gray-800 transition hover:text-gray-500 dark:text-gray-200 dark:hover:text-gray-400"
								aria-controls="hs-basic-with-title-and-arrow-stretched-collapse-two"
							>
								My team has credits. How do we use them?
								<svg
									class="hs-accordion-active:hidden block w-3 h-3 text-gray-600 group-hover:text-gray-500 dark:text-gray-400"
									width="16"
									height="16"
									viewBox="0 0 16 16"
									fill="none"
									xmlns="http://www.w3.org/2000/svg"
								>
									<path
										d="M2 5L8.16086 10.6869C8.35239 10.8637 8.64761 10.8637 8.83914 10.6869L15 5"
										stroke="currentColor"
										stroke-width="2"
										stroke-linecap="round"></path>
								</svg>
								<svg
									class="hs-accordion-active:block hidden w-3 h-3 text-gray-600 group-hover:text-gray-500 dark:text-gray-400"
									width="16"
									height="16"
									viewBox="0 0 16 16"
									fill="none"
									xmlns="http://www.w3.org/2000/svg"
								>
									<path
										d="M2 11L8.16086 5.31305C8.35239 5.13625 8.64761 5.13625 8.83914 5.31305L15 11"
										stroke="currentColor"
										stroke-width="2"
										stroke-linecap="round"></path>
								</svg>
							</button>
							<div
								id="hs-basic-with-title-and-arrow-stretched-collapse-two"
								class="hs-accordion-content hidden w-full overflow-hidden transition-[height] duration-300"
								aria-labelledby="hs-basic-with-title-and-arrow-stretched-heading-two"
							>
								<p class="text-gray-800 dark:text-gray-200">
									Once your team signs up for a subscription plan. This is where
									we sit down, grab a cup of coffee and dial in the details.
								</p>
							</div>
						</div>

						<div
							class="hs-accordion hs-accordion-active:bg-gray-100 rounded-xl p-6 dark:hs-accordion-active:bg-white/[.05]"
							id="hs-basic-with-title-and-arrow-stretched-heading-three"
						>
							<button
								class="hs-accordion-toggle group pb-3 inline-flex items-center justify-between gap-x-3 w-full md:text-lg font-semibold text-left text-gray-800 transition hover:text-gray-500 dark:text-gray-200 dark:hover:text-gray-400"
								aria-controls="hs-basic-with-title-and-arrow-stretched-collapse-three"
							>
								How does Preline's pricing work?
								<svg
									class="hs-accordion-active:hidden block w-3 h-3 text-gray-600 group-hover:text-gray-500 dark:text-gray-400"
